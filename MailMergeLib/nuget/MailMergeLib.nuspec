			<dependency id="Microsoft.Bcl.Async" version="1.0.168" />
			<dependency id="Microsoft.Bcl.Build" version="1.0.21" />
		</group>
		<group targetFramework="net45">
			<dependency id="MailKit" version="1.6" />
			<dependency id="AngleSharp" version="0.9" />
		</group>
    </dependencies>
  </metadata>
  <files>
		<file src="..\bin\Release\net40\MailMergeLib.dll" target="lib\net40\MailKit.dll" />
		<file src="..\bin\Release\net40\MailMergeLib.xml" target="lib\net40\MailKit.xml" />
		<file src="..\bin\Release\net45\MailMergeLib.dll" target="lib\net45\MailKit.dll" />
